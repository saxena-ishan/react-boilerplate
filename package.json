{
    "name": "react-boilerplate",
    "version": "0.0.1",
    "description": "a simple react boilerplate",
    "main": "/src/app.jsx",
    "scripts": {
        "start": "npm run clean && webpack-dev-server --config ./build-config/webpack.dev.config.js",
        "build": "npm run clean && webpack --config ./build-config/webpack.config.js",
        "deploy": "npm run build && gh-pages -d build",
        "clean": "rm -Rf ./build/bundle.js ./build/index.html",
        "reset": "npm run clean && rm -Rf ./node_modules ./package-lock.json"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "core-js": "^3.1.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.2",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.5.0",
        "@babel/preset-env": "^7.5.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.0.0",
        "gh-pages": "^2.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-harddisk-plugin": "^1.0.1",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.12.0",
        "redux-devtools-extension": "^2.13.8",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.35.2",
        "webpack-cli": "^3.3.5",
        "webpack-dev-server": "^3.7.2"
    }
}
